<h1><%= @list.title %></h1>
<h3><%= @user.name %></h3>
<p>
  <%= @list.description %>
</p>
<% @list.questions.each do |question| %>
  <%= question.title %>
  <%= render 'questions/like', { question: question } %>
  <br>
<% end %>
<h2>Post a question</h2>
<%= render 'questions/form' %>
<br>
<%= link_to 'Edit', edit_list_path(@list) %><br>
<%= link_to 'Back', root_url %>
