
<h1>Create a session</h1>
<%= button_to 'Create a session', new_list_path, method: 'get', class: 'waves-effect waves-light btn' %>
<br>
<h1>Current sessions running</h1>
<ul class="collection">
  <% @lists.each do |list| %>
    <li class="collection-item">
    <%= link_to list do %>
      <h5><%= list.title %></h5>
      <!-- <%= link_to 'Show', list_path(list) %>,
      <%= link_to 'Edit', edit_list_path(list) %>,
      <%= link_to 'Delete', list_path(list), method: :delete, data: { confirm: "Are you sure?" } %> -->
    <% end %>
  <% end %>
  </li>
</ul>
