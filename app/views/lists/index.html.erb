
  <h1>Create a session</h1>
  <%= link_to 'Create a session', new_list_path %>
  <br>
  <h1>Current sessions running</h1>
  <ul>
  <% @lists.each do |list| %>
    <li>
      <%= list.title %> |
      <%= link_to 'Show', list_path(list) %>,
      <%= link_to 'Edit', edit_list_path(list) %>,
      <%= link_to 'Delete', list_path(list), method: :delete, data: { confirm: "Are you sure?" } %>
    </li>
  <% end %>
  </ul>
