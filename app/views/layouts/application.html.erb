<!DOCTYPE html>
<html>
  <head>
    <title>Qna</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="wrapper">
      <%= render 'layouts/header' %>
      <div class="content">
        <div class="container">
          <div class="content">
            <%= render 'layouts/flash' %>
            <%= yield %>
          </div>
        </div>
      </div>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
